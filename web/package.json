{
  "name": "euphrates-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "wasm": "nix build ..#wasm && jco transpile ../result/lib/euphrates.wasm -o src/lib/wasm --instantiation"
  },
  "devDependencies": {
    "@bytecodealliance/jco": "^1.13.3",
    "@bytecodealliance/preview2-shim": "github:bytecodealliance/jco#path:packages/preview2-shim",
    "@molarmanful/fe-tools": "^0.2.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.33.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.2",
    "@unocss/eslint-config": "66.4.2",
    "@unocss/extractor-svelte": "66.4.2",
    "@unocss/preset-wind4": "^66.4.2",
    "eslint": "^9.33.0",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "unocss": "^66.4.2",
    "vite": "^7.1.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
